<div class="row">
	<div class="col-md-offset-8 col-md-4">

		<%= form_for  @user do |f|%>
		<%= render 'shared/error_messages', object: f.object %>
		<%= f.label :photo %>
		<%= f.file_field :photo %>
		
		<%= render 'form', f: f%>

		<%= f.submit "Save changes", class: "btn btn-primary" %>
		<%- end %>	
	</div>
	
</div>